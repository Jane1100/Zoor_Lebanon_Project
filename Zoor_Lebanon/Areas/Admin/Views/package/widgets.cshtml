            <section class="content">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Package List</h3>
                        <div class="card-tools">
                            <a href="~/admin/package/addpackage" class="btn btn-primary btn-sm">Add New Package</a>
                        </div>
                    </div>
                    <div class="card-body">
                            @if (Model != null && Model.Any())
                            {
                                <table class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th>Package Name</th>
                                            <th>Description</th>
                                            <th>Unit Price</th>
                                            <th>Start Date</th>
                                            <th>Total Spots</th>
                                            <th>Location</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var package in Model)
                                        {
                                            <tr>
                                                <td>@package.PackageName</td>
                                                <td>@package.Description</td>
                                                <td>@package.UnitPrice</td>
                                                <td>@package.StartDate</td>
                                                <td>@package.TotalSpots</td>
                                                 <td>@package.Location.City</td>
                                                <td>
                                                    <a href="@Url.Action("EditPackage", new { id = package.PackageId })" class="btn btn-info btn-sm">
                                                    <i class="fas fa-pencil-alt">
                                                    </i>
                                                    Edit
                                                    </a>
                                                <form asp-area="Admin" asp-controller="Package" asp-action="DeletePackage" method="post" style="display:inline">
                                                    <input type="hidden" name="id" value="@package.PackageId" />
                                                    <button type="submit" class="btn btn-danger btn-sm">
                                                        <i class="fas fa-trash"></i> Delete
                                                    </button>
                                                </form>



                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            }
                            else
                            {
                                <div class="alert alert-warning">
                                    No packages available. <a href="@Url.Action("AddPackage", "Package")">Click here to add a package</a>.
                                </div>
                            }

                    </div>
                </div>
            </section>
